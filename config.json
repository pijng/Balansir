{
    "server_list": [
        {"endpoint": "127.0.0.1:5000", "weight": 0.5},
        {"endpoint": "127.0.0.1:5001", "weight": 0.5}
    ],
    "connection_protocol": "http",
    "autocert": true,
    "autocert_dir": "./certs",
    "white_hosts": ["example.com", "anotherone.com"],
    "ssl_certificate": "./server.crt",
    "ssl_private_key": "./server.key",
    "http_port": 1337,
    "https_port": 443,
    "read_timeout": 10,
    "write_timeout": 20,
    "server_check_timer": 1,
    "server_check_response_timeout": 5,
    "session_persistence": false,
    "session_max_age": 300,
    "rate_limit": true,
    "rate_per_second": 100,
    "rate_bucket": 300,
    "proxy_mode": "transparent",
    "balancing_algorithm": "weighted-least-connections"
}