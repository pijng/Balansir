{
    "server_list": [
        {"endpoint": "127.0.0.1:5000", "weight": 0.5}
    ],
    "connection_protocol": "http",
    "autocert": false,
    "autocert_dir": "./certs",
    "white_hosts": ["example.com", "anotherone.com"],
    "ssl_certificate": "./server.crt",
    "ssl_private_key": "./server.key",
    "http_port": 1447,
    "https_port": 443,
    "read_timeout": 5,
    "write_timeout": 10,
    "server_check_timer": 1,
    "server_check_response_timeout": 1,
    "session_persistence": false,
    "session_max_age": 300,
    "gzip_response": false,
    "rate_limit": true,
    "rate_per_second": 200,
    "rate_bucket": 450,
    "transparent_proxy_mode": false,
    "balancing_algorithm": "weighted-least-connections",
    "cache": true,
    "cache_shards_amount": 1,
    "cache_shard_max_size_mb": 2,
    "cache_algorithm": "LFU",
    "cache_shard_exceed_fallback": false,
    "cache_background_update": true,
    "cache_rules": [
        {"path": "/static/", "ttl": "10.Second"}
    ]
}